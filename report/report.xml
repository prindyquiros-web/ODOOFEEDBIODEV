<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="report_contrato_pdf" model="ir.actions.report">
        <field name="name">Contrato PDF</field>
        <field name="model">x.contrato</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">feedbio_provision_custom.report_contrato_pdf_template</field>
        <field name="print_report_name">'Contrato - %s' % (object.name or '')</field>
        <!-- puedes cambiar el paperformat si quieres uno personalizado -->
        <field name="paperformat_id" ref="base.paperformat_euro"/>
    </record>

</odoo>
